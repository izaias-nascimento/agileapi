{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    //no-use-before-define": "off",
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error",
            { "argsIgnorePattern": "_" }
          ]
    
    }
}
